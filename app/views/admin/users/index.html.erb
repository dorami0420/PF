
<h1>会員一覧</h1>

<% @users.each do |user| %>

<div>
<p>氏名</p>
 <%= link_to admin_user_path(user.id) do %>
 <%= user.last_name + " " + user.first_name %>
 <% end %>

<p>メールアドレス</p>
 <%= user.email %>

<p>ステータス</p>

 <% if user.is_deleted %>
   <p>無効</p>

 <% else %>
    <p>有効</p>
 <% end %>

</div>

 <% end %>