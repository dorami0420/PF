<h1>ProtectiveCats#new</h1>
<h1>画像投稿フォーム</h1>
<%= form_with model: @protective_cat do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
 <h4>色</h4>
  <%= f.text_area :color %>
  <h4>性別</h4>
  <%= f.text_area :gender %>
  <h4>避妊/去勢</h4>
  <%= f.text_area :neuter %>
  <h4>健康状態</h4>
  <%= f.text_area :health %>
  
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>
<% end %>